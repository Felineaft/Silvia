    <script type="text/javascript">
        $(window).on("load", function () {
            /* Carregar Full-Banners randomicos */
            CarregarFullBanners(1);

            setTimeout(function () {
                /* Carregar Full-Banners não randomicos */
                CarregarFullBanners(0);
            }, 250);

            setTimeout(function () {
                /* Marcas da vitrine. */
                LoadingHomeBrands();
            }, 250);

            /* Oferta Relampago */
            if ($("#carouselOfertaRelampago").length > 0) {
                CarregamentoInicialOfertaRelampago();

                setTimeout(function () {
                    /* Produtos da Vitrine */
                    CarregarProdutosIniciaisVitrine();
                }, 250);
            } else {
                /* Produtos da Vitrine */
                CarregarProdutosIniciaisVitrine();
            }

            setTimeout(function () {
                /* Carregar Half-Banners randomicos */
                CarregarHalfBanners(1);

                /* Carregar Half-Banners não randomicos */
                CarregarHalfBanners(0);
            }, 250);

            setTimeout(function () {
                /* Grupos da Vitrine */
                CarregarGruposVitrine();
            }, 250);

            setTimeout(function () {
                /* Rodapé. */
                CarregarRodapeLoja();
            }, 250);

		    $.fn.matchHeight._maintainScroll = true;
		});
    </script>
