	<script type="text/javascript">
		function redirectPostTarget(target, location, args) {
			var form = '';
			$.each(args, function(key, value) {
				form += '<input type="hidden" name="' + key + '" value="' + value + '">';
			});
			$('<form action="' + location + '" method="POST" target="' + target + '">' + form + '</form>').appendTo('body').submit();
		}

		$(window).on("load", function () {
			redirectPostTarget('iframecontrol', 'https://cosmetologiaavancada.minhalojanouol.com.br/sys/templates/default/utilities/sessao_trans_iframe.asp?dgb=1&t=ScVDCmxiGDj9fYnVNpi8rg%3D%3D&u=r8uXRhG5ZKTaWmHT0WLEdsbQJ5qICzRk45uNp1QERF%2FN7oPkpzQ54A%3D%3D&i=qg7xgrbcehc%3D', {});
		});
	</script>

